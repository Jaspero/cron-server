{
  "name": "cron-server",
  "version": "0.0.0-development",
  "main": "index.js",
  "scripts": {
    "db": "mongod",
    "build:server": "npm --prefix server run build",
    "watch:server": "npm --prefix server run watch",
    "start:server": "npm --prefix server run start",
    "start:server:watch": "npm --prefix server run start:watch",
    "build": "rm -rf dist && npm run build:server && cp LICENSE dist/ && cp README.md dist/ && cp server/package.json dist/ && cp server/package-lock.json dist/",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Jaspero/cron-server.git"
  },
  "author": "Jaspero",
  "bugs": {
    "url": "https://github.com/Jaspero/cron-server/issues"
  },
  "homepage": "https://github.com/Jaspero/cron-server#readme",
  "devDependencies": {
    "semantic-release": "17.4.4"
  },
  "release": {
    "pkgRoot": "dist",
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/github",
      "@semantic-release/git"
    ]
  },
  "dependencies": {
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/git": "9.0.0"
  }
}
